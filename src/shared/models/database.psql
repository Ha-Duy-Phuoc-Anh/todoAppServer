CREATE TABLE todos (
    id SERIAL PRIMARY KEY,
    user_name VARCHAR(150) NOT NULL,
    task VARCHAR(255) NOT NULL,
    complete BOOLEAN,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);

CREATE TABLE users (
    id SERIAL PRIMARY KEY,
    user_name VARCHAR(150) NOT NULL,
    hashed_password VARCHAR(255) NOT NULL,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);